::_::
t+=.01
for i=0,990 do
	x=rnd(127)
	y=rnd(127)
	p=x;o=y;w=80+10*s(t)
	x+=s(o/w+t)*9
	y+=c(p/w-t)*9
	q=60+30*c(t/2)
	v=mid(8,11.3+(s(x/q+t-x/q/2)*.7+s(t/4)*.3+c(y/q-t-y/q/2)*.7+c(t/4)*.3)*1.7,14)
	fillp()
