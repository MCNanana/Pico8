-- manage score
function manage_score(e)
	if(cntwave[1]!=e.wave)then
	 cntwave[1]=e.wave
	 cntwave[2]=e.wavenb
	 cntwave[3]=1
	 score+=scrwave
	 scrwave=0
	else
	 scrwavetimer+=60
	 cntwave[3]+=1
	 scrwave+=flr(50*cntwave[3]/cntwave[2])
	end
end

