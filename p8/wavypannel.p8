__lua__
--scrolling text wavy pannel
--pico-8 doodle by trasevol_dog


r=rnd
palt(0,false)

drk={[0]=0,0,1,1,2,1,5,6,2,4,9,3,1,1,2}

cls()

str="!p\73\67\79-8 d\79\79\68\76\69!"
str="!hi this is a pico-8 doodle!"
str="!h\73 t\72\73\83 i\83 \65 pico-8 doodle!"

print(str,1,3,7)
memcpy(0x0,0x6000,0x1000)

t=0
function _draw()
 cls()

 t+=0.01

 w=12

 for i=0,(w-1) do
 for j=0,(w-1) do
  local sx=(flr(t*20))%#str*4
  local c=sget(i+sx,j)
 
  local x={}
  local y={}
  for k=1,4 do
   local l=k-1
   x[k]=(i+l%2)*128/w
   y[k]=(j+flr(l/2))*128/w
   
   x[k]=x[k]+4*cos(t+x[k]/10+y[k]/10)
   y[k]=y[k]+4*sin(t+0.62+x[k]/10+y[k]/10)

  end
  
  trifill(x[1],y[1],x[2],y[2],x[3],y[3],c)
  trifill(x[2],y[2],x[3],y[3],x[4],y[4],c)
  
 end
 end
 
 --trifill(16,16,16,96,96,16,7)
 

end





function tri(x1,y1,x2,y2,x3,y3,c)
 line(x1,y1,x2,y2,c)
 line(x2,y2,x3,y3,c)
 line(x1,y1,x3,y3,c)
end


function trifill(x1,y1,x2,y2,x3,y3,c)
 
 local hy=max(y1,max(y2,y3))
 local hx
 if y1==hy then hx=x1
 elseif y2==hy then hx=x2
 else hx=x3 end
 
 local ly=min(y1,min(y2,y3))
 local lx
 if y1==ly then lx=x1
 elseif y2==ly then lx=x2
 else lx=x3 end
 
 local my=mid(y1,y2,y3)
 local mx
 if y1==my and x1~=hx and x1~=lx then mx=x1
 elseif y2==my and x2~=hx and x2~=lx then mx=x2
 else mx=x3 end
 
 
 local k=my-ly
 for i=0,k do
  local xa,xb,y
  y=ly+i
  xa=lx+(i/k)*(mx-lx)
  xb=lx+(i/(hy-ly))*(hx-lx)
  
  --if c>0 then c=flr(y/4+t*10)%3*2+8 end
  cc=flr(y/8+t*10)%3+8
  if c==0 then cc=drk[cc+4] end
  
  line(xa,y,xb,y,cc)
 end
 
 local k2=hy-my
 for i=0,k2 do
  local xa,xb,y
  y=my+i
  xa=mx+(i/k2)*(hx-mx)
  xb=lx+((i+k)/(hy-ly))*(hx-lx)
  
  cc=flr(y/8+t*10)%3+8
  if c==0 then cc=drk[drk[cc]] end
  
  line(xa,y,xb,y,cc)
 end

 --line(x1,y1,x2,y2,7)
 --line(x2,y2,x3,y3,7)
 --line(x1,y1,x3,y3,7)

end
