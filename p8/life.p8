pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--life
--pico-8 doodle by trasevol_dog


pts={}
links={}

camx=0
camy=0

function _init()
 local pt1=create_point(64,64,{})
 local pt2=create_point(64,64,{pt1})
 local pt3=create_point(64,64,{pt1,pt2})

 local pt1=create_point(64,64,{})
 local pt2=create_point(64,64,{pt1})
 local pt3=create_point(64,64,{pt1,pt2})
end

function _update()

 local moyx=0
 local moyy=0
 
 for p in all(pts) do
  moyx+=0.1*p.x
  moyy+=0.1*p.y
 end
 
 moyx/=#pts*0.1
 moyy/=#pts*0.1
 
 camx=0.9*camx+0.1*(moyx-64)
 camy=0.9*camy+0.1*(moyy-64)
 
 
 for l in all(links) do
  l.tested=false
 end
 
 for l in all(links) do
  update_link(l)
 end
 
 for p in all(pts) do
  update_point(p)
 end
 
end

function _draw()
 cls()
 
 camera(camx,camy)

 for l in all(links) do
  line(l.p1.x,l.p1.y,l.p2.x,l.p2.y,7)
 end
 
 for p in all(pts) do
  circ(p.x,p.y,1,9)
 end
 
-- print(stat(0),0,0,6)
-- print(stat(1),0,8,6)

end


function update_point(p)
 p.x+=p.vx
 p.y+=p.vy
 p.vx*=0.8
 p.vy*=0.8
 
 local used=false
 for l in all(links) do
  if l.p1==p or l.p2==p then
   used=true
  end
 end
 
 if not used then
  del(pts,p)
 end
end

function update_link(l)
 l.frs=max(l.frs-0.01,0)
 
 local p1,p2=l.p1,l.p2
 local len=sqr(p1.x-p2.x)+sqr(p1.y-p2.y)
 local a=atan2(p1.x-p2.x,p1.y-p2.y)

 local f=0.5
 if len<sqr(l.l-1) then
  p1.vx+=f*cos(a)
  p1.vy+=f*sin(a)
  p2.vx-=f*cos(a)
  p2.vy-=f*sin(a)
 end
 
 if len>sqr(l.l+1) then
  p1.vx-=f*cos(a)
  p1.vy-=f*sin(a)
  p2.vx+=f*cos(a)
  p2.vy+=f*sin(a)
 end
 
 l.tested=true
 if l.frs==0 then
 for l2 in all(links) do
  if not (l2.tested or l2.frs>0 or l.p1==l2.p1 or l.p2==l2.p2 or l.p1==l2.p2 or l.p2==l2.p1) then
   local crs=detect_inters(l.p1,l.p2,l2.p1,l2.p2)
   
   if crs then
    create_point(crs.x,crs.y,{l.p1,l.p2,l2.p1,l2.p2})
    del(links,l)
    del(links,l2)
    return
   end
  end
 end
 end

end


function create_point(x,y,linked)
 --if #pts>99 then return end

 local a=rnd(1)
 
 local p={
  x=x,
  y=y,
  vx=0.1*cos(a),
  vy=0.1*sin(a)
 }
 
 for l in all(linked) do
  create_link(p,l)
 end
 
 if #pts>100 then
  --delete_point(pts[flr(rnd(#pts)+1)])
  delete_point(pts[#pts])
  delete_point(pts[1])
 end
 
 add(pts,p)
 
 return p
end

function create_link(p1,p2)
 local len=nil
 if abs(p1.x-p2.x)>2 and abs(p1.y-p2.y)>2 then
  len=sqrt(sqr(p1.x-p2.x)+sqr(p1.y-p2.y))
 end

 local l={
  p1=p1,
  p2=p2,
  l=len or 30+rnd(4),
  frs=0.1
 }
 
 add(links,l)
end


function delete_point(p)
 for l in all(links) do
  if l.p1==p or l.p2==p then
   del(links,l)
  end
 end
 
 del(pts,p)
end



function detect_inters(p1,p2,p3,p4)

 local a1=(p2.y-p1.y)/(p2.x-p1.x)
 local a2=(p4.y-p3.y)/(p4.x-p3.x)

 local b1=p1.y-a1*p1.x
 local b2=p3.y-a2*p3.x
 
 local sx=(b2-b1)/(a1-a2)

 if mid(p1.x,p2.x,sx)==sx and mid(p3.x,p4.x,sx)==sx then
  return {x=sx,y=a1*sx+b1}
 else
  return false
 end
end


function sqr(a) return a*a end
