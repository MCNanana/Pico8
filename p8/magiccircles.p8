pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--magic circles
--pico-8 doodle by trasevol_dog

--inspired by onor mccann


drk={[0]=0,0,1,1,2,1,5,6,2,4,9,3,1,1,8,9}

cls()

kbls=16

klin=0
lincor={}

ka=0

bls={}
for i=1,kbls do
 bls[i]={
  x=64,
  y=64,
  aimx=64,
  aimy=64
 }
end

t=0
function _update()

 if t%0.5<0.01 then
  ka+=0.1+rnd(0.8)
  
  rm=flr(rnd(2))*(4+flr(rnd(8)))
  
  am=flr(rnd(2))*(1/kbls/flr(rnd(4)+1))
  
  for i=1,kbls do
   local r=48+rm*cos(i/2)
   local a=ka+i/kbls+am*cos(i/2)
   bls[i].aimx=64+r*cos(a)
   bls[i].aimy=64+r*sin(a)
  end
  
  klin=flr(rnd(min(kbls/2,8)))+1
  for i=1,klin do
   lincor[i]=flr(rnd(kbls-1)+1)
  end
 end
 
 for b in all(bls) do
  b.x=0.8*b.x+0.2*b.aimx
  b.y=0.8*b.y+0.2*b.aimy
 end

 t+=0.01
end


function _draw()

 --cls()
 
 spr(0,0,0,15,15)
 
 for i=0,999 do
  local x,y=rnd(128),rnd(128)
  local c=pget(x,y)
  
  if c==7 then
   c=rnd(6)+8
  elseif rnd(2)<0.5 then
    c=0
  end
  
  circ(x,y,1,drk[flr(c)])
  
  sset(x-1,y,c)
  sset(x+1,y,c)
  sset(x,y-1,c)
  sset(x,y+1,c)
 end
 

 if abs(bls[1].x-bls[1].aimx)<4 then

 for i=1,kbls do
  local x1,y1=bls[i].x,bls[i].y
 
  for j=1,klin do
   local id=(i+lincor[j]-1)%kbls+1
   local x2,y2=bls[id].x,bls[id].y
   
   --local c=(j)%6+8
   
   line(x1,y1,x2,y2,7)
  end
 
 end
 
 end
 
 for b in all(bls) do
  circfill(b.x,b.y,2,0)
  circ(b.x,b.y,2,7)
 end

end
