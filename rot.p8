pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
angle,tr=0,0
scale=1
::_::
cls(0)
	ca,sa=cos(angle),sin(angle)
	tr+=.001
	--scale=abs(cos(tr*3))*8+1
	angle-=.01

--local col=0
local x=0
local y=0
local xp=0
local yp=0
for x=-30,30 do
	for y=-30,30 do
		xp=((x*ca+y*sa)+(4*scale))/scale
		yp=((-x*sa+y*ca)+(4*scale))/scale
		if((xp>=0)and(xp<8)
				and(yp>=0)and(yp<8))then
			pset(60+x,60+y,sget(16+xp,yp))
		end
	end
end
flip()
goto _

