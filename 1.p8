-- draw --
function _draw()
 cls()
 --draw_rotation()
 draw_starfield()
 --draw_text()
	draw_water()
	
	--[[for i=0,4095 do
		pset(i%128,128-i/128,fire[i].c)
		fire[i].c=fire[i+1].c
		fire[i].v=fire[i+1].v		
	end
	print(spd)]]
	print("made by mcnanana",1,120,5)
end

cam_x = 0
cam_y = 0
-----------
-- water --
function update_water()
  if (btn(0) and cam_x > 0) cam_x -= 1
  if (btn(1) and cam_x < 895) cam_x += 1
  if (btn(2) and cam_y > 0) cam_y -= 1
  if (btn(3) and cam_y < 127) cam_y += 1
  -- (the camera stops with the bottom of
  -- the screen at row 32.)
end

function draw_water()	
  -- set the camera to the current location
  camera(cam_x, cam_y)

  -- draw the entire map at (0, 0), allowing
  -- the camera and clipping region to decide
  -- what is shown
  map(0, 0, 0, 0, 128, 128)

  -- reset the camera then print the camera
  -- coordinates on screen
  camera()
  print('('..cam_x..', '..cam_y..')', 0, 0, 7)	
end

--------------
-- rotation --
function update_rotation()
	ca,sa=cos(angle),sin(angle)
	t+=0.001
	scale=abs(cos(t*1.3))*5+1
	angle-=0.005
end

function draw_rotation()
	local col=0
	local x=0
	local y=0
	local xp=0
	local yp=0
	for x=31,96 do
		for y=31,96 do
			xp=(x*ca+y*sa)/scale
			yp=(-x*sa+y*ca)/scale
			pset(x,y,sget(24+(xp%8),yp%8))
		end
	end
end

----------
-- text --
function draw_text()
	local tx,ty=0,0
	local txt,tyt=0,0
	for j=0,7  do
		ta=0
		for i=0,48 do
			local col=sget(stx+i,8+j)
			if (col!=0) then
				txt=cos(ta+(i+j)*0.035)
				tyt=sin(ta+(i+j)*0.02)
				tx=10+i*8+txt*(i+j)
				ty=32+j*8+tyt*(i+j)
				local s=0
				if(tyt<-0.8) then s=2
				else if(tyt<-0.45) then s=1
				else s=0 end
				end
				--s=3+2*txt+2.25*tyt
				spr(s,tx,ty)
				print(tyt,10,10+6*j,5)
				--pset(x,y,bufcolor[x+y*128])
			end
			ta+=5
		end
	end
	stx+=0.1
	if (stx>40) stx=-20
end

---------------
-- starfield --
function draw_starfield()
	foreach(str,
		function(v)
			--[[if (v.c==10) then
				spr(0,v.x,v.y)
			else]]
				pset(v.x,v.y,v.c)
			--end
			v.x-=(v.s+spd)
			if(v.x<0) then
				v.x+=128
				v.y=rnd(128)
			end
		end)
end

-- menu
function showmenu()
	rectfill(32,10,96,100,1)
 print("rotation:",40,20,13)
 --print("rotation:"..t,40,20,13)
end
