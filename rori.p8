pico-8 cartridge // http://www.pico-8.com
version 15
__lua__
t=0
x=63;y=14
missile=false
xm=0;ym=0
xd=63

function _update()
	t+=1
	if(btn(0)) x-=.8
	if(btn(1)) x+=.8
	if(btn(2)) y-=.5
	if(btn(3)) y+=.5
	if(btn(4)) missile=true;xm=x;ym=y+8
	-- vaisseau
	if(x<0)then x=127
	elseif(x>127)then x=0 
	end
	if(y<0)then y=127
	elseif(y>127)then y=0
	end
	if(missile==true)then
		xm-=.0;ym+=1
	end
	-- dame
end

function _draw()
	cls(1)
	-- vaisseau
	spr(3,x,y)
	if(missile==true) spr(5,xm,ym)
	-- dame
	spr(1,xd,102)
	if(t<10)then	spr(2,xd,110)
	elseif(t<20)then spr(18,xd,110)
	else spr(18,xd,110);t=0	
	end
end

